<div>
    <h2>Admin Users Page</h2>
    <% users.forEach(user => { %> 
        <div style="margin: 30px;background-color: antiquewhite;">
            <h1><%= user.username %></h1>
            <p><%= user.email %></p>
            <p><%= user.phone %></p>
            <p><%= user.guest_type %></p>
        </div>
    <% }) %> 
</div>